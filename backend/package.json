{
  "name": "amazon-listing-optimizer-backend",
  "version": "1.0.0",
  "description": "Backend API for Amazon Product Listing Optimizer",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
  "dev": "cd ../frontend && npm run build && cp -r dist ../backend/public && cd ../backend && nodemon src/server.js",
    "migrate": "node migrations/migrate.js up",
    "migrate:status": "node migrations/migrate.js status",
    "migrate:reset": "mysql -u root -e 'DROP DATABASE IF EXISTS amazon_optimizer' && node migrations/migrate.js up",
    "lint": "eslint src/**/*.js migrations/**/*.js --max-warnings=0",
    "lint:fix": "eslint src/**/*.js migrations/**/*.js --fix",
    "format": "prettier --write \"src/**/*.js\" \"migrations/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.js\" \"migrations/**/*.js\""
  },
  "keywords": [
    "amazon",
    "listing",
    "optimizer",
    "ai",
    "gemini"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mysql2": "^3.6.0",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "nodemon": "^3.0.1",
    "prettier": "^3.6.2"
  }
}
